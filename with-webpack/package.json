{
  "name": "bundling-with-webpack",
  "author": "henriqueDerosa",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build:prod": "export NODE_ENV=production && webpack --config ./webpack/webpack.config.js",
    "build:dev": "export NODE_ENV=development && webpack --config ./webpack/webpack.config.js",
    "dev": "export NODE_ENV=development && webpack serve --config ./webpack/webpack.config.js --open"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "babel-loader": "^8.2.5",
    "babel-plugin-import": "^1.13.5",
    "css-loader": "^6.7.1",
    "html-webpack-plugin": "^5.5.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.72.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0",
    "webpack-graphql-loader": "^1.0.2"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.6.2",
    "@mui/material": "^5.6.4",
    "graphql-request": "^4.2.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  }
}
